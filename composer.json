{
    "name": "noah-medra/prompt-builder",
    "version": "1.0.0",
    "description": "Laravel package to build dynamic AI prompts",
    "type": "library",
    "autoload": {
      "psr-4": {
        "NoahMedra\\PromptBuilder\\": "src/"
      }
    },
    "require": {
      "php": "^8.0",
      "illuminate/support": "^12.0",
      "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.0",
      "mockery/mockery": "^1.0",
        "pestphp/pest": "*",
        "laravel/pint": "*",
        "orchestra/testbench": "*"
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "scripts": {
      "post-autoload-dump": [
        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
        "@php artisan package:discover --ansi"
      ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
